<% for(var i = 0; i < cafes.length; i++) { %>
<tr>
    <td><%= i + 1 %></td>
    <td><%= cafes[i].name %></td>
    <td><%= cafes[i].summary %></td>
    <td><%= cafes[i].tag %></td>
    <td><%= cafes[i].lat %></td>
    <td><%= cafes[i].lon %></td>
    <td>
        <a href="/update-user?id=<%= cafes[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fas fa-pencil-alt"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id=<%= cafes[i]._id%> >
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>
<% } %>